{
  "name": "civicship-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "dotenvx run -f .env --env='NODE_HTTPS=false' -- tsx watch src/index.ts",
    "dev:https": "dotenvx run -f .env --env='NODE_HTTPS=true' -- tsx watch src/index.ts",
    "build": "tsc && tsc-alias tsconfig.json",
    "lint": "eslint --fix src/ && prettier --write src/",
    "test": "jest",
    "db:pull": "prisma db pull --schema src/prisma/schema.prisma",
    "db:seed-master": "tsx src/prisma/seeds/index.ts --master",
    "db:seed-domain": "tsx src/prisma/seeds/index.ts --domain",
    "db:generate": "prisma-case-format --file src/prisma/schema.prisma && prisma format --schema src/prisma/schema.prisma && prisma generate --sql --schema src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema src/prisma/schema.prisma --create-only",
    "db:deploy": "prisma migrate deploy --schema src/prisma/schema.prisma",
    "db:mark-rolled-back": "prisma migrate resolve --schema src/prisma/schema.prisma --rolled-back",
    "db:studio": "prisma studio --schema src/prisma/schema.prisma",
    "gql:generate": "graphql-codegen --config codegen.yaml",
    "doc": "typedoc",
    "container:up": "ls src/prisma/migrations/*/migration.sql | sort | awk -F'/' '{print \"cp \"$0\" ./container/docker/postgresql/init/\"$(NF-2)\"_\"$(NF-1)\"_\"$(NF)}' | bash | docker-compose up -d",
    "container:down": "docker-compose down --rmi all --volumes --remove-orphans"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^9.4.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@graphql-markdown/docusaurus": "^1.26.0",
    "@mermaid-js/mermaid-cli": "^10.9.1",
    "@ngneat/falso": "^7.2.0",
    "@prisma/client": "^6.2.1",
    "@prisma/generator-helper": "^5.19.1",
    "@quramy/jest-prisma": "^1.8.1",
    "@quramy/prisma-fabbrica": "^2.2.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.5.2",
    "@types/node-fetch": "^2.6.11",
    "babel-jest": "^29.7.0",
    "dbdocs": "^0.12.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "graphql-schema-linter": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^6.2.1",
    "prisma-case-format": "^2.2.1",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-erd-generator": "^1.11.2",
    "prisma-json-types-generator": "^3.0.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.0",
    "typedoc": "^0.26.6",
    "typedoc-plugin-merge-modules": "^6.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@dotenvx/dotenvx": "^1.14.0",
    "@google-cloud/logging-winston": "^6.0.0",
    "@graphql-tools/graphql-file-loader": "^8.0.12",
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.7",
    "@graphql-tools/schema": "^10.0.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.13",
    "express": "^4.19.2",
    "firebase-admin": "^12.4.0",
    "graphql": "^16.9.0",
    "graphql-constraint-directive": "^6.0.0",
    "graphql-limiter": "^1.3.0",
    "graphql-middleware": "^6.1.35",
    "graphql-query-complexity": "^1.0.0",
    "graphql-scalars": "^1.23.0",
    "graphql-shield": "^7.6.5",
    "ioredis": "^5.5.0",
    "node-fetch": "^3.3.2",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.14.2"
  }
}
