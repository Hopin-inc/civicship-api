{
  "name": "civicship-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "tsx watch src/index.ts -r tsconfig-paths/register",
    "build": "tsc && tsc-alias tsconfig.json",
    "lint": "eslint --fix src/ && prettier --write src/",
    "test": "jest",
    "db:pull": "prisma db pull --schema src/prisma/schema.prisma",
    "db:seed": "tsx src/prisma/seeds/index.ts",
    "db:generate": "prisma-case-format --file src/prisma/schema.prisma && prisma format --schema src/prisma/schema.prisma && prisma generate --schema src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema src/prisma/schema.prisma --create-only",
    "db:deploy": "prisma migrate deploy --schema src/prisma/schema.prisma",
    "db:mark-rolled-back": "prisma migrate resolve --schema src/prisma/schema.prisma --rolled-back",
    "db:studio": "prisma studio --schema src/prisma/schema.prisma",
    "gql:generate": "graphql-codegen --config codegen.yaml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.8",
    "@graphql-codegen/typescript-operations": "^4.2.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@mermaid-js/mermaid-cli": "^10.9.1",
    "@prisma/generator-helper": "^5.16.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prisma": "^5.16.1",
    "prisma-case-format": "^2.2.1",
    "prisma-erd-generator": "^1.11.2",
    "prisma-json-types-generator": "^3.0.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.4",
    "@graphql-tools/schema": "^10.0.4",
    "@prisma/client": "^5.16.1",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.11",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0"
  }
}
