{
  "name": "civicship-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "copy-graphql": "copyfiles -u 1 'src/**/*.graphql' 'dist'",
    "build": "tsc && tsc-alias tsconfig.json && pnpm copy-graphql",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "dotenvx run -f .env --env='NODE_HTTPS=false' -- tsx watch src/index.ts",
    "dev:https": "dotenvx run -f .env --env='NODE_HTTPS=true' -- tsx watch src/index.ts",
    "lint": "eslint --fix src/ && prettier --write src/",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "db:pull": "prisma db pull --schema src/infrastructure/prisma/schema.prisma",
    "db:seed-master": "tsx src/infrastructure/prisma/seeds/index.ts -- --master",
    "db:seed-domain": "tsx src/infrastructure/prisma/seeds/index.ts -- --domain",
    "db:generate": "prisma-case-format --file src/infrastructure/prisma/schema.prisma && prisma format --schema src/infrastructure/prisma/schema.prisma && prisma generate --sql --schema src/infrastructure/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema src/infrastructure/prisma/schema.prisma --create-only",
    "db:deploy": "prisma migrate deploy --schema src/infrastructure/prisma/schema.prisma",
    "db:mark-rolled-back": "prisma migrate resolve --schema src/infrastructure/prisma/schema.prisma --rolled-back",
    "db:studio": "prisma studio --schema src/infrastructure/prisma/schema.prisma",
    "gql:generate": "graphql-codegen --config codegen.yaml",
    "container:up": "ls src/infrastructure/prisma/migrations/*/migration.sql | sort | awk -F'/' '{print \"cp \"$0\" ./container/docker/postgresql/init/\"$(NF-2)\"_\"$(NF-1)\"_\"$(NF)}' | bash | docker-compose up -d",
    "container:down": "docker-compose down --rmi all --volumes --remove-orphans"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.25.1",
    "@faker-js/faker": "^9.7.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@graphql-codegen/typescript-resolvers": "^4.5.0",
    "@mermaid-js/mermaid-cli": "^10.9.1",
    "@ngneat/falso": "^7.3.0",
    "@prisma/client": "^6.6.0",
    "@prisma/generator-helper": "^5.22.0",
    "@quramy/jest-prisma": "^1.8.1",
    "@quramy/prisma-fabbrica": "^2.2.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.2",
    "@types/node-fetch": "^2.6.12",
    "@types/sanitize-html": "^2.15.0",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^29.7.0",
    "copyfiles": "^2.4.1",
    "dbdocs": "^0.12.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.15.0",
    "graphql-schema-linter": "^3.0.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "prisma-case-format": "^2.2.1",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-erd-generator": "^2.0.4",
    "prisma-json-types-generator": "^3.3.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.3",
    "tsyringe": "^4.10.0",
    "typedoc-plugin-merge-modules": "^6.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@dotenvx/dotenvx": "^1.41.0",
    "@escape.tech/graphql-armor": "^3.1.3",
    "@escape.tech/graphql-armor-types": "^0.7.0",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.16.0",
