{
  "name": "civicship-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "dotenvx run -f .env --env='NODE_HTTPS=false' -- tsx watch src/index.ts",
    "dev:https": "dotenvx run -f .env --env='NODE_HTTPS=true' -- tsx watch src/index.ts",
    "build": "tsc && tsc-alias tsconfig.json",
    "lint": "eslint --fix src/ && prettier --write src/",
    "test": "jest",
    "db:pull": "prisma db pull --schema src/infrastructure/prisma/schema/main.prisma",
    "db:seed-master": "tsx src/infrastructure/prisma/seeds/index.ts --master",
    "db:seed-domain": "tsx src/infrastructure/prisma/seeds/index.ts --domain",
    "db:generate": "prisma-case-format --file src/infrastructure/prisma/schema/main.prisma && prisma format --schema src/infrastructure/prisma/schema && prisma generate --sql --schema src/infrastructure/prisma/schema",
    "db:migrate": "prisma migrate dev --schema src/infrastructure/prisma/schema --create-only",
    "db:deploy": "prisma migrate deploy --schema src/infrastructure/prisma/schema",
    "db:mark-rolled-back": "prisma migrate resolve --schema src/infrastructure/prisma/schema --rolled-back",
    "db:studio": "prisma studio --schema src/infrastructure/prisma/schema",
    "gql:generate": "graphql-codegen --config codegen.yaml",
    "doc": "typedoc",
    "container:up": "ls src/infrastructure/prisma/migrations/*/migration.sql | sort | awk -F'/' '{print \"cp \"$0\" ./container/docker/postgresql/init/\"$(NF-2)\"_\"$(NF-1)\"_\"$(NF)}' | bash | docker-compose up -d",
    "container:down": "docker-compose down --rmi all --volumes --remove-orphans"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.22.0",
    "@faker-js/faker": "^9.6.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@graphql-codegen/typescript-resolvers": "^4.4.4",
    "@graphql-markdown/docusaurus": "^1.27.0",
    "@mermaid-js/mermaid-cli": "^10.9.1",
    "@ngneat/falso": "^7.3.0",
    "@prisma/client": "^6.4.1",
    "@prisma/generator-helper": "^5.22.0",
    "@quramy/jest-prisma": "^1.8.1",
    "@quramy/prisma-fabbrica": "^2.2.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/node-fetch": "^2.6.12",
    "@types/sanitize-html": "^2.13.0",
    "babel-jest": "^29.7.0",
    "dbdocs": "^0.12.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.15.0",
    "graphql-schema-linter": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.4.1",
    "prisma-case-format": "^2.2.1",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-erd-generator": "^2.0.4",
    "prisma-json-types-generator": "^3.2.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typedoc": "^0.26.11",
    "typedoc-plugin-merge-modules": "^6.1.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@dotenvx/dotenvx": "^1.38.4",
    "@google-cloud/logging-winston": "^6.0.0",
    "@graphql-tools/graphql-file-loader": "^8.0.17",
    "@graphql-tools/load": "^8.0.17",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.0.22",
    "@graphql-tools/schema": "^10.0.21",
    "@line/bot-sdk": "^9.7.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dataloader": "^2.2.3",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "firebase-admin": "^12.7.0",
    "graphql": "^16.10.0",
    "graphql-constraint-directive": "^6.0.0",
    "graphql-limiter": "^1.3.0",
    "graphql-middleware": "^6.1.35",
    "graphql-query-complexity": "^1.0.0",
    "graphql-scalars": "^1.24.1",
    "graphql-shield": "^7.6.5",
    "node-fetch": "^3.3.2",
    "sanitize-html": "^2.14.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "puppeteer",
      "vue-demi"
    ]
  }
}
