{
  "name": "civicship-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "export NODE_HTTPS=false && tsx watch src/index.ts -r tsconfig-paths/register",
    "dev:https": "export NODE_HTTPS=true && tsx watch src/index.ts -r tsconfig-paths/register",
    "build": "tsc && tsc-alias tsconfig.json",
    "lint": "eslint --fix src/ && prettier --write src/",
    "test": "jest",
    "db:pull": "prisma db pull --schema src/prisma/schema/relation.prisma",
    "db:seed": "tsx src/prisma/seeds/index.ts",
    "db:generate": "prisma-case-format --file src/prisma/schema/relation.prisma && prisma format --schema src/prisma/schema.prisma && prisma generate --schema src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema src/prisma/schema/relation.prisma --create-only",
    "db:deploy": "prisma migrate deploy --schema src/prisma/schema/relation.prisma",
    "db:mark-rolled-back": "prisma migrate resolve --schema src/prisma/schema/relation.prisma --rolled-back",
    "db:studio": "prisma studio --schema src/prisma/schema/relation.prisma",
    "gql:generate": "graphql-codegen --config codegen.yaml",
    "docs": "typedoc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@eslint/js": "^9.7.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@graphql-markdown/docusaurus": "^1.26.0",
    "@mermaid-js/mermaid-cli": "^10.9.1",
    "@ngneat/falso": "^7.2.0",
    "@prisma/generator-helper": "^5.17.0",
    "@quramy/jest-prisma": "^1.8.1",
    "@quramy/prisma-fabbrica": "^2.2.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "babel-jest": "^29.7.0",
    "dbdocs": "^0.10.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.8.0",
    "graphql-schema-linter": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "prisma-case-format": "^2.2.1",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-erd-generator": "^1.11.2",
    "prisma-json-types-generator": "^3.0.4",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.2",
    "typedoc": "^0.26.5",
    "typedoc-plugin-merge-modules": "^6.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "dependencies": {
    "@apollo/server": "^4.10.5",
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.4",
    "@graphql-tools/schema": "^10.0.4",
    "@prisma/client": "^5.17.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.12",
    "express": "^4.19.2",
    "firebase-admin": "^12.3.0",
    "graphql": "^16.9.0",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.23.0",
    "graphql-shield": "^7.6.5"
  }
}
